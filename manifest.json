{
	"manifest_version": 2,
	"name": "News Improver\u2122",
	"version": "1.1",
	"description": "Improves the news significantly.",
	"homepage_url": "http://www.xkcd.com/1288",
	"content_scripts":
	[
		{
			"all_frames": true,
			"matches": ["*://*/*"],
			"js": ["improve.js"],
			"run_at": "document_end"
		}
	],
	"icons":
	{
		"16":"xkcd16.png",
		"48": "xkcd48.png",
		"128": "xkcd128.png"
	}

}

